<asf xmlversion="1.0">	

		<module type="component" id="qtlib.generic.tmx.qm.avrxmega" caption="QTouch Project for AVR XMEGA group">

			<require idref="qtouch.generic.include.touch_api.qtextension"/>				    	
		
			<require idref="qtouch.generic.avr8.qmatrix.common.xmega.qtextension"/>
			<require idref="qtouch.qdebug.qtextension"/>	
		
			<build type="include-path" value="conf" />
			<build type="header-file" subtype="api" value="conf\conf_clock.h" />
			<build type="header-file" subtype="api" value="conf\conf_sleepmgr.h" />
			<build type="header-file" subtype="api" value="conf\QDebugSettings.h" />
			<require idref="qtouch.templates_init.device"/>
			<require idref="qtouch.generic.avr8.qtouch.examples.xmega.qtextension"/>			
					  
			<require-external idref="common.services.basic.clock" eidref="atmel.asf" />
			<require-external idref="xmega.drivers.cpu" eidref="atmel.asf" />
			<require-external idref="xmega.drivers.tc" eidref="atmel.asf" />		
			
			<device-support value="atxmega128a1" />
			<device-support value="atxmega16a4" />
			<device-support value="atxmega32a4" />
			<device-support value="atxmega16d4" />
			<device-support value="atxmega32d4" />
			<device-support value="atxmega32d4" />

			<require idref="qtouch.qtextension.license" />
		
			<info type="keyword" value="technology">
				<keyword value="Touch"/>
			</info>
		</module>
</asf>
