<asf xmlversion="1.0">
	<project caption="QTouch method Example Application for ATxmega128A1" id="common.services.qtouch.generic.avr8.qtouch.examples.example_qt_qt600.xmega.qtextension.project">
		<info type="description" value="summary">
		This QTouch Library Example demonstrates usage of the QTouch method and QDebug on QT600 board.
		A QT600 Bridge is used as interface with Qtouch Analyser tools.
		</info>
		
		<build type="c-source" value="main.c" />
		<build type="include-path" value="."/>
		
		<require idref="qtouch.generic.avr8.qtouch.examples.xmega.conf.qtextension" />
		<require idref="common.services.qtouch.generic.avr8.qtouch.examples.example_qt_qt600.xmega.qtextension" />
		<require idref="config.qtouch.generic.avr8.xmega128a1_qt600.method"/>
		<config name="config.qtouch.generic.avr8.xmega128a1_qt600.method" value="qtouch_method"/>
		<require-external idref="board.xmega128a1_qt600" eidref="atmel.asf" />		

		<device-support value="atxmega128a1" />

		<generator value="avrgcc"/>
		<generator value="as5_8"/>
		<generator value="doxygen"/>
		
		<require idref="qtouch.qtextension.license" />
	</project>		
</asf>
