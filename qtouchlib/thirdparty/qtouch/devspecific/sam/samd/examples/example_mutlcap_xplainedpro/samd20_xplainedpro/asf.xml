<asf xmlversion="1.0">
	<project caption="SAM D20 Xplained Pro and QT1 Xplained Pro Mutual Capacitance example application" id="common.services.qtouch.devspecific.sam.samd.examples.example_qm_xplainedpro.samd20_xplainedpro.qtextension.project">
